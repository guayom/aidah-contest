<%= simple_form_for(@participant_rating) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :participant %>
    <%= f.association :category %>
    <%= f.association :parameter %>
    <%= f.input :score %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
